<ui:UiBinder
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:sb='urn:import:com.webex.jcasandra.board.client.view'>

  <g:DockLayoutPanel unit='EM'>
    <g:north size='8'>
      <sb:TopPanelView ui:field='topPanel' />
    </g:north>

    <g:center>
      <g:SplitLayoutPanel>
        <g:west size='240'>
          <sb:StatusListView ui:field='statusList' />
        </g:west>

        <g:center>
          <sb:StatusDetailView ui:field='detail' />
        </g:center>
      </g:SplitLayoutPanel>
    </g:center>
  </g:DockLayoutPanel>

</ui:UiBinder>
